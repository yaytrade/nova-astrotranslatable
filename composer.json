{
  "name": "kraenkvisuell/nova-astrotranslatable",
  "description": "An Astrotomic Translatable extension for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "translatable",
    "i18n",
    "translate"
  ],
  "authors": [
    {
      "name": "Optimist Digital",
      "homepage": "https://github.com/optimistdigital"
    },
    {
      "name": "Kraenk Visuell",
      "homepage": "https://github.com/kraenkvisuell"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "astrotomic/laravel-translatable": "^11.5",
    "laravel/nova": "^4.12",
    "nova-kit/nova-packages-tool": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "KraenkVisuell\\NovaAstrotranslatable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KraenkVisuell\\NovaAstrotranslatable\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
